{
  "name": "@repo/contracts",
  "version": "0.0.1",
  "main": "index.ts",
  "types": "index.ts",
  "license": "MIT",
  "scripts": {
    "test:thor-solo": "hardhat test --network vechain_solo test/integration/*.test.ts",
    "test:hardhat": "hardhat test --network hardhat test/unit/**/*.test.ts",
    "test:coverage:solidity": "hardhat coverage --testfiles 'test/unit/**/*.test.ts'",
    "coverage:report": "nyc report --reporter=lcov --reporter=text --reporter=html --temp-dir=./coverage",
    "compile": "npx hardhat compile",
    "deploy": "npx hardhat run scripts/deploy/select-and-deploy.ts --network vechain_solo",
    "deploy:solo": "npx hardhat run scripts/deploy --network vechain_solo",
    "deploy:testnet": "npx hardhat run scripts/deploy --network vechain_testnet",
    "deploy:mainnet": "npx hardhat run scripts/deploy --network vechain_mainnet",
    "deploy:contract:solo": "npx hardhat run scripts/deploy/deployments/\"$CONTRACT_TO_DEPLOY\"/\"$CONTRACT_TO_DEPLOY\"-\"$CONTRACT_VERSION\".ts --network vechain_solo",
    "deploy:contract:testnet": "npx hardhat run scripts/deploy/deployments/\"$CONTRACT_TO_DEPLOY\"/\"$CONTRACT_TO_DEPLOY\"-\"$CONTRACT_VERSION\".ts --network vechain_testnet",
    "deploy:contract:mainnet": "npx hardhat run scripts/deploy/deployments/\"$CONTRACT_TO_DEPLOY\"/\"$CONTRACT_TO_DEPLOY\"-\"$CONTRACT_VERSION\".ts --network vechain_mainnet",
    "upgrade:contract": "npx hardhat run scripts/upgrade/upgrades/\"$CONTRACT_TO_UPGRADE\"/\"$CONTRACT_TO_UPGRADE\"-\"$CONTRACT_VERSION\".ts --network vechain_solo",
    "upgrade:contract:testnet": "npx hardhat run scripts/upgrade/upgrades/\"$CONTRACT_TO_UPGRADE\"/\"$CONTRACT_TO_UPGRADE\"-\"$CONTRACT_VERSION\".ts --network vechain_testnet",
    "upgrade:contract:mainnet": "npx hardhat run scripts/upgrade/upgrades/\"$CONTRACT_TO_UPGRADE\"/\"$CONTRACT_TO_UPGRADE\"-\"$CONTRACT_VERSION\".ts --network vechain_mainnet",
    "check-contracts-deployment": "npx hardhat run scripts/config/validateGenesisAndContracts.ts --network vechain_solo",
    "check-contracts-deployment:devnet": "npx hardhat run scripts/config/validateGenesisAndContracts.ts --network vechain_devnet",
    "check-contracts-deployment:testnet": "npx hardhat run scripts/config/checkContractsDeployment.ts --network vechain_testnet",
    "check-contracts-deployment:mainnet": "npx hardhat run scripts/config/checkContractsDeployment.ts --network vechain_mainnet",
    "generate-stargate-nft-metadata": "npx hardhat run scripts/metadata/StargateNFT/generate.ts",
    "upload-stargate-nft-metadata": "yarn generate-stargate-nft-metadata && npx hardhat run scripts/metadata/StargateNFT/upload.ts",
    "solo-up": "ts-node scripts/thor-solo/startThorSolo.ts",
    "solo-down": "docker compose down",
    "solo-clean": "docker compose down -v --remove-orphans",
    "wait-for-thor-solo": "ts-node scripts/thor-solo/waitForThorSolo.ts",
    "mine-blocks": "npx hardhat run scripts/thor-solo/mineBlocks.ts --network vechain_solo",
    "fast-forward-periods": "npx hardhat run scripts/thor-solo/fastForwardPeriods.ts --network vechain_solo",
    "clean": "rm -rf dist artifacts typechain-types cache node_modules",
    "format": "prettier --check \"**/*.{ts,js,sol,md}\"",
    "format:write": "prettier --write \"**/*.{ts,js,sol,md}\"",
    "lint": "eslint . --ext .ts",
    "generate-docs": "npx hardhat docgen",
    "purge": "yarn clean",
    "token-manager:get-migration-data:solo": "VERBOSE=true npx hardhat run scripts/token-manager/getTokenManagerMigrationData.ts --network vechain_solo",
    "token-manager:get-migration-data:devnet": "VERBOSE=true npx hardhat run scripts/token-manager/getTokenManagerMigrationData.ts --network vechain_devnet",
    "token-manager:get-migration-data:testnet": "VERBOSE=true npx hardhat run scripts/token-manager/getTokenManagerMigrationData.ts --network vechain_testnet",
    "token-manager:get-migration-data:mainnet": "VERBOSE=true npx hardhat run scripts/token-manager/getTokenManagerMigrationData.ts --network vechain_mainnet",
    "token-manager:migrate:solo": "VERBOSE=true npx hardhat run scripts/token-manager/migrateNodesFromJson.ts --network vechain_solo",
    "token-manager:migrate:devnet": "VERBOSE=true npx hardhat run scripts/token-manager/migrateNodesFromJson.ts --network vechain_devnet",
    "token-manager:migrate:testnet": "VERBOSE=true npx hardhat run scripts/token-manager/migrateNodesFromJson.ts --network vechain_testnet",
    "token-manager:migrate:mainnet": "VERBOSE=true npx hardhat run scripts/token-manager/migrateNodesFromJson.ts --network vechain_mainnet",
    "hayabusa:get-claim-base-rewards-data:solo": "VERBOSE=true npx hardhat run scripts/claim-base-rewards/get-claim-base-rewards-data.ts --network vechain_solo",
    "hayabusa:get-claim-base-rewards-data:testnet": "VERBOSE=true npx hardhat run scripts/claim-base-rewards/get-claim-base-rewards-data.ts --network vechain_testnet",
    "hayabusa:get-claim-base-rewards-data:mainnet": "VERBOSE=true npx hardhat run scripts/claim-base-rewards/get-claim-base-rewards-data.ts --network vechain_mainnet",
    "hayabusa:claim-base-rewards:solo": "VERBOSE=true npx hardhat run scripts/claim-base-rewards/claim-base-rewards-from-json.ts --network vechain_solo",
    "hayabusa:claim-base-rewards:testnet": "VERBOSE=true npx hardhat run scripts/claim-base-rewards/claim-base-rewards-from-json.ts --network vechain_testnet",
    "hayabusa:claim-base-rewards:mainnet": "VERBOSE=true npx hardhat run scripts/claim-base-rewards/claim-base-rewards-from-json.ts --network vechain_mainnet",
    "hayabusa:get-claim-delegation-rewards-data:solo": "VERBOSE=true npx hardhat run scripts/claim-delegation-rewards/get-claim-delegation-rewards-data.ts --network vechain_solo",
    "hayabusa:get-claim-delegation-rewards-data:testnet": "VERBOSE=true npx hardhat run scripts/claim-delegation-rewards/get-claim-delegation-rewards-data.ts --network vechain_testnet",
    "hayabusa:get-claim-delegation-rewards-data:mainnet": "VERBOSE=true npx hardhat run scripts/claim-delegation-rewards/get-claim-delegation-rewards-data.ts --network vechain_mainnet",
    "hayabusa:claim-delegation-rewards:solo": "VERBOSE=true npx hardhat run scripts/claim-delegation-rewards/claim-delegation-rewards-from-json.ts --network vechain_solo",
    "hayabusa:claim-delegation-rewards:testnet": "VERBOSE=true npx hardhat run scripts/claim-delegation-rewards/claim-delegation-rewards-from-json.ts --network vechain_testnet",
    "hayabusa:claim-delegation-rewards:mainnet": "VERBOSE=true npx hardhat run scripts/claim-delegation-rewards/claim-delegation-rewards-from-json.ts --network vechain_mainnet",
    "hayabusa-dryrun:prepare-environment": "VERBOSE=true npx hardhat run scripts/hayabusa-dryrun/prepare-environment.ts --network vechain_solo",
    "get-block-timestamp": "VERBOSE=true npx hardhat run scripts/timestamp/get-block-timestamp.ts"
  },
  "dependencies": {
    "@nomicfoundation/hardhat-chai-matchers": "^2.0.2",
    "@nomicfoundation/hardhat-ethers": "^3.0.4",
    "@nomicfoundation/hardhat-network-helpers": "^1.0.10",
    "@nomiclabs/hardhat-etherscan": "^3.0.0",
    "@nomiclabs/hardhat-truffle5": "^2.0.7",
    "@nomiclabs/hardhat-web3": "^2.0.0",
    "@openzeppelin/contracts": "5.0.2",
    "@openzeppelin/contracts-upgradeable": "5.0.2",
    "@openzeppelin/upgrades-core": "^1.32.5",
    "@repo/config": "*",
    "@repo/utils": "*",
    "@typechain/hardhat": "^8.0.0",
    "@types/chai": "^4.2.0",
    "@types/mocha": ">=9.1.0",
    "@vechain/sdk-core": "2.0.3",
    "@vechain/sdk-hardhat-plugin": "2.0.3",
    "@vechain/sdk-network": "2.0.3",
    "@vechain/web3-providers-connex": "^1.1.2",
    "archiver": "^7.0.1",
    "axios": "^1.7.2",
    "chai": "^4.2.0",
    "dotenv": "^16.3.1",
    "form-data": "^4.0.0",
    "fs": "^0.0.1-security",
    "glob": "^10.3.10",
    "hardhat": "^2.22.15",
    "hardhat-contract-sizer": "^2.10.0",
    "hardhat-gas-reporter": "^1.0.8",
    "hardhat-ignore-warnings": "^0.2.11",
    "inquirer": "^12.4.2",
    "nyc": "^17.1.0",
    "solidity-coverage": "^0.8.11",
    "solidity-docgen": "^0.6.0-beta.36",
    "ts-node": ">=8.0.0",
    "typechain": "^8.1.0",
    "typescript": ">=4.5.0"
  },
  "devDependencies": {
    "@inquirer/prompts": "^7.5.0",
    "@nomicfoundation/hardhat-toolbox": "^3.0.0",
    "@nomicfoundation/hardhat-verify": "2.1.0",
    "@typechain/ethers-v6": "^0.4.3",
    "@types/archiver": "^6.0.2",
    "csv-writer": "^1.6.0",
    "eslint": "^8.56.0",
    "eslint-plugin-only-warn": "^1.1.0",
    "ethers": "^6.9.0",
    "nft.storage": "^7.1.1",
    "prettier": "^3.2.4",
    "prettier-plugin-solidity": "^2.1.0",
    "testcontainers": "^11.5.1"
  }
}
